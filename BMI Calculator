height = float(input('Enter Your height in centimeter : '))
weight  = float(input('Enter you weight in kg :'))
height = height/100
BMI = weight/(height*height)
print("You Body Mass Index : ",BMI)
if(BMI>0):
    if(BMI<=16):
        print('You are several underweight')
    elif(BMI<=18.5):
        print('you are umderweight')
    elif(BMI<=25):
        print('you are heilty')
    elif(BMI<=35):
        print('You are overweight')
    else:print('enter valid details')
